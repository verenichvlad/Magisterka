  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Meteor Server",
      "cwd": "${workspaceRoot}/",
      "runtimeExecutable": "${workspaceRoot}/.meteor/local/dev_bundle/bin/npm",
      "restart": true,
      "timeout": 30000,
      "stopOnEntry": false,
      "sourceMaps": true,
      "protocol": "inspector",
      "port": 9229
    }
  ]
}